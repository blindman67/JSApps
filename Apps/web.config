<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <caching>
            <profiles>
                <remove extension=".js" />
                <remove extension=".jsm" />
                <add extension=".jsm" policy="DisableCache" kernelCachePolicy="DisableCache" duration="00:00:30" />
                <add extension=".js" policy="DisableCache" kernelCachePolicy="DisableCache" duration="00:00:30" />
            </profiles>
        </caching>
    </system.webServer>
</configuration>
